package leetcode;

import java.util.HashMap;
import java.util.List;

public class BtPhoneNumber {
	public List<String>letterCombine(String digits) {
		HashMap<Character, String> map = new HashMap<>();

		map.put('2', "abc");
		map.put('3', "def");
		map.put('4', "ghi");
		map.put('5', "jkl");
		map.put('6', "mno");
		map.put('7', "pqrs");
		map.put('8', "tuv");
		map.put('9', "wxyz");

		backTrack();

		return null;
	}
	
	public void backTrack() {
		
	}
	public static void main(String[] args) {
		String digits = "27";
		BtPhoneNumber p1=new BtPhoneNumber();

		System.out.println(p1.letterCombine(digits));
	}

}
