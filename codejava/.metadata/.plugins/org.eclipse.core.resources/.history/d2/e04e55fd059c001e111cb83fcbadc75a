package leetcode;

public class AddTwoNodes{
	
	private class Node{
		private int data;
		public int getData() {
			return data;
		}
		public void setData(int data) {
			this.data = data;
		}
		public Node getNext() {
			return next;
		}
		public void setNext(Node next) {
			this.next = next;
		}
		private Node next;
	}
	
	Node head;
	public void add(int data) {
		Node container = new Node();
		container.setData(data);
		if (head == null) {
			head = container;
		} else {
			Node tempNode = head;
			while (tempNode.getNext()!= null) {
				tempNode = tempNode.getNext();
			}
			tempNode.setNext(container);
		}
	}
	
	public void addNode(Node a,Node b) {
		
	}
	public static void main(String[] args) {
		AddTwoNodes n1=new AddTwoNodes();
		AddTwoNodes n2=new AddTwoNodes();
		AddTwoNodes n3=new AddTwoNodes();

		n1.add(5);
		n1.add(6);
		n1.add(7);
		
		n2.add(8);
		n2.add(9);
		n2.add(10);
		
		
	}

}
