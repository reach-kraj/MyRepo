package leetcode;
class Animal {
	  public void makeNoise() {
	    System.out.println("Animal sound");
	  }
	}

	class Dog extends Animal {
	  @Override
	  public void makeNoise() {
	    System.out.println("Woof");
	    // Call the superclass method
	    super.makeNoise();
	  }
	}
public class Main {
	 public static void main(String[] args) {
		    Dog dog = new Dog();
		    dog.makeNoise(); // Output: Woof Animal sound
		  }
}
